{
  "expo": {
    "scheme": "vision-gpt",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "name": "VisionGPT",
    "slug": "vision-gpt",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/newSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mertdonmez.visiongpt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/newIcon.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mertdonmez.visiongpt"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "675b8399-b6fd-43ef-a2be-07db430a1169"
      }
    }
  }
}
